code","source":["# File: notebooks/pagerank_ui.ipynb - Cell 2\n","\n","def run_pagerank_analysis(input_edge_list_path: str, output_analysis_path: str):\n","    log_stream = io.StringIO()\n","    logger = ConsoleAndGradioLogger(log_stream, logger_name=\"PageRankLogger\")\n","\n","    try:\n","        config = PageRankConfig(\n","            input_edge_list_path=input_edge_list_path,\n","            output_analysis_path=output_analysis_path\n","        )\n","\n","        logger.info(\"Initializing PageRank Processor...\")\n","        # Inject dependencies\n","        url_processor_instance = URLProcessor()\n","        processor = PageRankProcessor(\n","            config=config,\n","            logger=logger,\n","            url_processor=url_processor_instance,\n","            graph_analyzer_class=GraphAnalyzer # Pass the class, not an instance\n","        )\n","\n","        yield \"Step 1/2: Processing graph data...\", log_stream.getvalue(), None\n","        processor.process_graph_data()\n","\n","        final_status = \"âœ… PageRank Analysis Completed Successfully!\"\n","        logger.info(final_status)\n","\n","        # Load the saved results to display a summary in Gradio\n","        if os.path.exists(config.output_analysis_path):\n","            results_df = pd.read_csv(config.output_analysis_path)\n","            total_nodes = len(results_df)\n","            summary_md = f\"## Analysis Finished\\n\\n- **Status:** {final_status}\\n- **Output File:** `{config.output_analysis_path}`\\n- **Total URLs Analyzed (Nodes):** {total_nodes}\\n\\n### Top 5 URLs by PageRank:\\n\\n\"\n","            summary_md += results_df.sort_values(by='PageRank', ascending=False).head(5).to_markdown(index=False)\n","        else:\n","            summary_md = f\"## Analysis Finished\\n\\n- **Status:** {final_status}\\n- Could not load results for summary (output file not found).\"\n","\n","        yield final_status, log_stream.getvalue(), summary_md\n","\n","    except FileNotFoundErr